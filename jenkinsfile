pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
            stage('build') {
            steps {
                echo 'Building'
            }
            }
            stage('deploy') {
            steps {
                echo 'Deploying'
            }
            }
            stage('test') {
            steps {
                echo 'Testing'
            }
            }
            stage('release') {
            steps {
                echo 'Releasing'
            }
            }
        }
    }
